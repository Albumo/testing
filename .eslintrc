{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "globals": {
    "require": true
  },
  "rules": {
    "strict": 0,
    "no-undef": 0,
    "no-unused-vars": 0,
    "no-redeclare": 0,
    "no-prototype-builtins": 0
  },
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true
  }
}
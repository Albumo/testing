{
    "parser": "babel-eslint",
    "extends": "airbnb-base",
    "globals": {
        "require": true
    },
    "rules": {
        "strict": 0,
        "no-undef": 0,
        "no-unused-vars": 0,
        "no-redeclare": 0,
        "no-prototype-builtins": 0,
        "linebreak-style": 0,
        "indent": 0,
        "no-var": 0,
        "prefer-arrow-callback": 0,
        "no-use-before-define": 0,
        "func-names": 0,
        "comma-dangle": 0,
        "no-mixed-operators": 0,
        "one-var": 0,
        "vars-on-top": 0,
        "no-plusplus": 0,
        "wrap-iife": 0,
        "prefer-rest-params": 0,
        "no-param-reassign": 0,
        "prefer-template": 0,
        "object-shorthand": 0,
        "no-shadow": 0,
        "prefer-destructuring": 0
    },
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true
    }
}
